{
  "parser": "typescript",
  "singleQuote": true,
  "semi": true,
  "trailingComma": "all",
  "tabWidth": 4,
  "printWidth": 120,
  "useTabs": false,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^@grammy/(.*)$", "^@grammyjs/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
