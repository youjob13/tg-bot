{
  "name": "annushka_nails_tg_bot",
  "version": "1.0.0",
  "description": "Telegram bot",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "precommit": "",
    "start:dev": "nodemon -r dotenv/config ./src/index.ts | bunyan --color",
    "start": "node -r dotenv/config ./dist/src/index.js",
    "lint": "eslint src/**/*.ts",
    "build": "tsc -b tsconfig.json && cp ./src/assets/language.json ./dist/src/assets/language.json",
    "pretty": "prettier --config .prettierrc.json --write \"src/**/*.ts\""
  },
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "author": "Danil_Rodionov",
  "license": "ISC",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@tsconfig/node18": "18.2.2",
    "@types/bunyan": "1.8.9",
    "@types/express": "4.17.18",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.0",
    "nodemon": "3.0.1",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@grammyjs/menu": "1.2.1",
    "@grammyjs/runner": "2.0.3",
    "@grammyjs/transformer-throttler": "1.2.1",
    "bunyan": "1.8.15",
    "cron": "3.0.0",
    "dayjs": "1.11.7",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "grammy": "1.19.0",
    "http-status-codes": "^2.3.0",
    "luxon": "3.4.3",
    "mongodb": "6.1.0",
    "telegram-inline-calendar": "2.0.6"
  }
}
